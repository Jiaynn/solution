{
  "extends": "@qiniu/build-config/portal",
  "srcDir": "src",
  "optimization": {
    "highQualitySourceMap": true,
    "errorOverlay": false,
    "transformDeps": ["tr46", "ansi-regex", "whatwg-url", "webidl-conversions"]
  },
  "entries": {
    "index": "src/index.tsx"
  },
  "pages": {
    "index": {
      "template": "src/index.ejs",
      "entries": "index",
      "path": "^\\/"
    }
  },
  "engines": {
    "builder": "^2.3.0"
  },
  "envVariables": {
    "process.env.TRACK_ANALYTICS": false,
    "process.env.CONFIG": {}
  }
}
