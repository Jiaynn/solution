{
  "root": true,
  "extends": [
    "@qiniu"
  ],
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [ ".js", ".jsx", ".ts", ".tsx" ],
        "moduleDirectory": [ "node_modules", "src/" ]
      }
    }
  },
  "rules": {
    "@typescript-eslint/naming-convention": "warn"
  }
}
