{
  "name": "qnweb-im",
  "version": "1.0.5",
  "main": "build/qnweb-im.umd.js",
  "module": "build/qnweb-im.esm.js",
  "types": "build/types/index.d.ts",
  "author": "Spencer17x <1253478653@qq.com>",
  "repository": {
    "type": "git",
    "url": ""
  },
  "files": [
    "build"
  ],
  "scripts": {
    "build:prod": "rollup -c rollup.config.ts --environment NODE_ENV:prod",
    "build": "rollup -c rollup.config.ts --environment NODE_ENV:prod",
    "build:test": "rollup -c rollup.config.ts --environment NODE_ENV:test",
    "build:dev": "rollup -c rollup.config.ts --environment NODE_ENV:dev",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "prepublishOnly": "npm run build",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx, \"src/**/*\""
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.2",
    "colors": "^1.4.0",
    "eslint": "^7.32.0",
    "inquirer": "8.1.1",
    "jest": "^27.0.4",
    "jest-config": "^27.0.4",
    "lodash.camelcase": "^4.3.0",
    "ora": "5.4.1",
    "prompt": "^1.1.0",
    "replace-in-file": "^6.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.51.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-uglify": "^6.0.4",
    "shelljs": "0.8.4",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "tslib": "^2.2.0",
    "typescript": "^4.3.2"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/*.{js,ts}"
    ]
  }
}
